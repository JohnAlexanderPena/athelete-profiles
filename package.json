{
  "name": "generator-start",
  "version": "1.0.0",
  "private": true,
  "main": "app.js",
  "scripts": {

    "client-install": "npm install --prefix client",
    "start": "node app.js",
    "server": "nodemon app",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run app\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "concurrently": "^5.0.0",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "express-validator": "^6.3.0",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "mongoose": "^5.7.14",
    "morgan": "~1.9.1"
  },
  "devDependencies": {
    "nodemon": "^2.0.1"
  }
}
